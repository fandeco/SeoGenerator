{
    "name": "webnitros/app",
    "license": "MIT",
    "version": "1.1.2",
    "authors": [
        {
            "name": "webnitros",
            "email": "info@bustep.ru"
        }
    ],
    "scripts": {
        "tests" : "phpunit --bootstrap bootstrap.php",
        "up:release": "gh release create \"v1.0.2\" --generate-notes"
    },
    "keywords": [
        "client"
    ],
    "require": {
        "php": ">=7.4",
        "symfony/dotenv": "^5.2",
        "wapmorgan/morphos": "^3.2",
        "guzzlehttp/guzzle": "^7.5",
        "cijic/phpmorphy": "^0.3.1",
        "fenom/fenom": "^2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "files": [
            "src/Foundation/helpers.php"
        ],
        "psr-4": {
            "SeoGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
